{
    "name": "inserve/metabase-api-php",
    "description": "A PHP wrapper for the Metabase API",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.7",
        "symfony/serializer": "^6.3|^7",
        "symfony/property-access": "^6.3|^7",
        "psr/log": "^3.0",
        "phpdocumentor/reflection-docblock": "^5.3",
        "lcobucci/jwt": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2",
        "squizlabs/php_codesniffer": "^3.7",
        "phpstan/phpstan": "^1.10",
        "vimeo/psalm": "^5.16"
    },
    "autoload": {
        "psr-4": {
            "Inserve\\MetabaseAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Inserve\\MetabaseAPI\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Inserve",
            "email": "dev@inserve.nl"
        }
    ],
    "minimum-stability": "stable"
}
